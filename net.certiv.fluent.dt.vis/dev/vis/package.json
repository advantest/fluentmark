{
  "name": "vis",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "run-p type-check build-only",
    "dev": "run-p serve watch",
    "build-only": "vite build",
    "format": "prettier --write \"src/**/*.{ts,vue,html}\"",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.ts --fix --ignore-path .gitignore",
    "serve": "vite",
    "type-check": "vue-tsc --noEmit",
    "watch": "onchange \"src/**/*.{ts,vue,html,scss}\" -- prettier --write --ignore-unknown {{changed}}"
  },
  "dependencies": {
    "@mdi/font": "7.0.96",
    "highlight.js": "^11.8.0",
    "js-beautify": "^1.14.7",
    "mathjax": "^3.2.2",
    "pinia": "^2.0.32",
    "pinia-plugin-persistedstate": "^3.1.0",
    "roboto-fontface": "*",
    "vue": "^3.2.47",
    "vue-native-websocket-vue3": "^3.1.7",
    "vue-prism-editor": "^2.0.0-alpha.2",
    "vue-router": "^4.1.6",
    "vuetify": "^3.2.3",
    "webfontloader": "^1.0.0"
  },
  "devDependencies": {
    "@babel/types": "^7.21.4",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/js-beautify": "^1.13.3",
    "@types/node": "^18.14.2",
    "@types/prismjs": "^1.26.0",
    "@types/webfontloader": "^1.6.35",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.34.0",
    "eslint-plugin-vue": "^9.9.0",
    "eslint-plugin-vuetify": "^2.0.0-beta.4",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "prettier": "^2.8.8",
    "prismjs": "^1.29.0",
    "sass": "^1.62.1",
    "typescript": "~4.8.4",
    "vite": "^4.1.4",
    "vite-plugin-vuetify": "^1.0.0",
    "vue-tsc": "^1.2.0"
  }
}
